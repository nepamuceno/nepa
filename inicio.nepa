func factorial(n):
    si n == 0:
        retorna 1
    retorna n * factorial(n - 1)

func fibonacci(n):
    si n <= 1:
        retorna n
    retorna fibonacci(n - 1) + fibonacci(n - 2)

# [EJECUCIÃ“N]
variable num = 5
imprime("Factorial de", num, "es:", factorial(num))

imprime("Serie Fibonacci (primeros 7):")
para i desde 0 hasta 6:
    imprime("F(", i, ") =", fibonacci(i))

